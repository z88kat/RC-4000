{
  "name": "rc-4000",
  "version": "1.0.0",
  "description": "Seiko RC-4000Watch Manager",
  "main": "index-electron.cjs",
  "module": "main.mjs",
  "type": "module",
  "scripts": {
    "cmd": "node index.js",
    "start": "electron-forge start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "electron": "./node_modules/electron/dist/electron index-electron.cjs --no-sandbox",
    "rebuild": "electron-rebuild -f -w serialport",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "help": "electron-packager . --help",
    "build": "electron-packager . RC-4000 --arch=x64 --overwrite --icon=src/icon-watch.ico --out=dist --asar --prune=true --ignore=node_modules/electron-*",
    "build-mac": "electron-packager . --platform=darwin --arch=x64 --overwrite --icon=src/icon-watch.ico --out=dist --asar --prune=true --ignore=node_modules/electron-*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z88kat/RC-4000.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/z88kat/RC-4000/issues"
  },
  "homepage": "https://github.com/z88kat/RC-4000#readme",
  "dependencies": {
    "@shoelace-style/shoelace": "^2.3.0",
    "chalk": "^5.2.0",
    "colors": "^1.4.0",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.3",
    "electron-rebuild": "^3.2.9",
    "electron-squirrel-startup": "^1.0.0",
    "esm": "^3.2.25",
    "flatpickr": "^4.6.13",
    "fs-extra": "^11.1.1",
    "moment": "^2.29.4",
    "prompts": "^2.4.2",
    "pubsub-js": "^1.9.4",
    "serialport": "^10.5.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.0",
    "@electron-forge/maker-deb": "^6.1.0",
    "@electron-forge/maker-rpm": "^6.1.0",
    "@electron-forge/maker-squirrel": "^6.1.0",
    "@electron-forge/maker-zip": "^6.1.0",
    "electron": "^23.1.4",
    "electron-packager": "^17.1.1"
  }
}